public class Solution {
    // DO NOT MODIFY THE ARGUMENTS WITH "final" PREFIX. IT IS READ ONLY
    public int findCount(final int[] A, int B) {
        int size = A.length;
        int count = 0;
        for (int i=0; i<size; i++){
            if (B == A[i])
                count++;
        }
        if(count==0){
            return 0;
        }
           
        return count;
    
    }
}
