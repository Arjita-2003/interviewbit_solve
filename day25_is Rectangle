public class Solution {
    public int solve(int a, int b, int c, int d) {
        if (a == b && a == c && a == d && c == d && b == c
            && b == d)
            return 1;
 
        else if (a == b && c == d)
            return 1;
        else if (a == d && c == b)
            return 1;
        else if (a == c && d == b)
            return 1;
        else
            return 0;
    }
}
